{
    "name": "arubacao/laravel-friendships",
    "description": "Manage Friendships/Relationships between Models in Laravel",
    "keywords": ["laravel", "Lumen", "friend-system", "friendships", "friends", "relationships", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "arubacao",
            "email": "arubacao@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "squizlabs/php_codesniffer": "2.*",
        "fzaninotto/faker": "~1.4",
        "laravel/laravel": "5.*",
        "graham-campbell/testbench": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Arubacao\\Friendships\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arubacao\\Tests\\Friendships\\": "tests/"
        }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.1-dev"
      }
    },
    "minimum-stability": "stable"
}
